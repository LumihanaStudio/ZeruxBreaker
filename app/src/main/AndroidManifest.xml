<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.edcan.zeruxbreaker">
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="GESQUARE"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="GESQUARE">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.MyGesture"
            android:label="@string/title_activity_my_gesture"></activity>
        <activity
            android:name=".activity.DownloadGesture"
            android:label="@string/title_activity_download_gesture"></activity>
        <activity
            android:name=".activity.MakeGesture"
            android:label="@string/title_activity_make_gesture"></activity>
    </application>

</manifest>
